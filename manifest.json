{
  "manifest_version": 3,
  "name": "Tab Extractor",
  "description": "Move matching tabs to a new window using the address bar omnibox command 'ex'.",
  "version": "1.0.1",
  "omnibox": {
    "keyword": "ex"
  },
  "background": {
    "service_worker": "TabExtractor.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Tab Extractor"
  }
}
